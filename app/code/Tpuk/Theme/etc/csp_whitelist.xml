<?xml version="1.0" encoding="UTF-8"?>
<csp_whitelist xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
               xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Csp:etc/csp_whitelist.xsd">
    <policies>
        <policy id="script-src">
            <values>
                <value id="fontawesome-script" type="host">kit.fontawesome.com</value>
                <value id="klarna-osm" type="host">x.klarnacdn.net</value>
            </values>
        </policy>
        <policy id="font-src">
            <values>
                <value id="google-font" type="host">fonts.gstatic.com</value>
            </values>
        </policy>
        <policy id="style-src">
            <values>
                <value id="googleapis" type="host">fonts.googleapis.com</value>
            </values>
        </policy>
    </policies>
</csp_whitelist>
